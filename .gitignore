*.log
logs/
debug.log

# Ignore IDE/editor settings
.vscode/
.idea/
*.swp
*.swo

# Ignore Node.js dependencies (If using React or frontend)
node_modules/
npm-debug.log

# Ignore static and media files
/media/
staticfiles/
static/

# Ignore compiled assets (If using webpack or collectstatic)
*.tar.gz
*.zip

# Ignore coverage reports (For testing)
htmlcov/
.coverage
*.cover
pytest_cache/

